extends ../base
include ../mixins/video 

block content 
    div.profile__data
        img(src=isHeroku ?  user.avatarUrl : "/" + user.avatarUrl,crossorigin).avatar
        h3=user.name
    div.video-grid
        each video in user.videos
            +videoMixins(video)
        else 
            span.empty__message No videos found